{
    "name": "short.ly",
    "version": "1.0.0",
    "description": "Production-ready URL shortener with analytics, caching, and cloud integration",
    "main": "src/main.js",
    "scripts": {
      "start": "node src/main.js",
      "dev": "nodemon src/main.js",
      "test": "jest --coverage",
      "lint": "eslint .",
      "format": "prettier --write '**/*.{js,json,md}'",
      "prepare": "husky install"
    },
    "engines": {
      "node": ">=16.0.0"
    },
    "keywords": [
      "url-shortener",
      "express",
      "mongodb",
      "redis",
      "aws-lambda"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
      "aws-sdk": "^2.1097.0",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "express-rate-limit": "^6.7.0",
      "express-validator": "^7.0.1",
      "helmet": "^6.1.5",
      "http-errors": "^2.0.0",
      "joi": "^17.9.1",
      "mongoose": "^7.0.3",
      "morgan": "^1.10.0",
      "nanoid": "^3.3.4",
      "node-cache": "^5.1.2",
      "redis": "^4.6.6",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^4.6.2",
      "winston": "^3.8.2",
      "axios": "^1.3.6",
      "ua-parser-js": "^1.0.35",
      "compression": "^1.7.4"
    },
    "devDependencies": {
      "eslint": "^8.38.0",
      "eslint-config-prettier": "^8.8.0",
      "eslint-plugin-jest": "^27.2.1",
      "eslint-plugin-node": "^11.1.0",
      "husky": "^8.0.3",
      "jest": "^29.5.0",
      "lint-staged": "^13.2.1",
      "nodemon": "^2.0.22",
      "prettier": "^2.8.7",
      "supertest": "^6.3.3"
    },
    "lint-staged": {
      "*.js": "eslint --cache --fix",
      "*.{js,json,md}": "prettier --write"
    }
  }